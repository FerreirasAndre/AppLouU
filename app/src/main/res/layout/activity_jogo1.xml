        android:layout_height="100dp"
        android:backgroundTint="@color/white"
        android:textColor="@color/black"
        android:fontFamily="@font/mclaren"
        android:textSize="32dp"
        android:text="L"
        app:layout_constraintEnd_toEndOf="parent"
